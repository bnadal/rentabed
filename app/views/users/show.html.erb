<h1>My ads</h1>
<br>

<% @ads.each do |ad|%>
<ul>
<li>
  <div class="card-product">
     <img src=<%= ad.image %>>
     <div class="card-product-infos">
  <h3<%= link_to "#{ad.title}", ad_path(ad.id)  %></h3>
  <p> renter:
     <%= if ad.bookings.last == nil
         "This bed is not rented"
          else  ad.bookings.last.user.email
          end %>
            
  </p>
      <p><%= link_to "Delete this ad", ad_path(ad), method: :delete%></p>
</li>
</ul>
<% end %>

